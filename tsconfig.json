{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": "matflow/frontend/src",
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules/vuetify/types"
    ],
    "types": [
      "webpack-env",
      "vuetify",
      "jest"
    ],
    "paths": {
      "@Classes/*": [
        "Classes/*"
      ],
      "@Controler/*": [
        "Controler/*"
      ],
      "@Memento/*": [
        "Memento/*"
      ],
      "@Model/*": [
        "Model/*"
      ],
      "@View/*": [
        "View/*"
      ],
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
  "include": [
    "matflow/frontend/src/**/*.ts",
    "matflow/frontend/src/**/*.tsx",
    "matflow/frontend/src/**/*.vue",
    "tests/frontendtests/**/*.ts",
    "tests/frontendtests/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}